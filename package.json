{
  "name": "ms-treeview",
  "version": "0.0.1",
  "scripts": {
    "start": "npm run build-demo && npm run tscd && concurrently \"npm run tsc:wd\" \"npm run lite\" ",
    "lite": "lite-server",
    "postinstall": "typings install",
    "build-demo": "npm run demosymlinks && npm run build && npm run tscd && cp .build/ms-treeview.embeded.js demo/.build/ms-treeview.embeded.js",
    "demosymlinks": "ln -fs ../node_modules ./demo && ln -fs ../typings ./demo",
    "build": "npm run tsc && node pack.js",
    "tsc": "tsc --declaration",
    "tscd": "tsc --project demo",
    "tsc:w": "tsc -w",
    "tsc:wd": "tsc --project demo -w",
    "typings": "typings"
  },
  "license": "ISC",
  "dependencies": {
    "@angular/common": "2.0.0-rc.1",
    "@angular/compiler": "2.0.0-rc.1",
    "@angular/core": "2.0.0-rc.1",
    "@angular/http": "2.0.0-rc.1",
    "@angular/platform-browser": "2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "2.0.0-rc.1",
    "@angular/router": "2.0.0-rc.1",
    "@angular/router-deprecated": "2.0.0-rc.1",
    "@angular/upgrade": "2.0.0-rc.1",
    "systemjs": "0.19.27",
    "core-js": "^2.4.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "^0.6.12",
    "angular2-in-memory-web-api": "0.0.9"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "gulp": "^3.9.1",
    "gulp-angular-embed-templates": "^2.2.0",
    "lite-server": "^2.2.0",
    "typescript": "^1.8.10",
    "typings": "^0.8.1"
  }
}
